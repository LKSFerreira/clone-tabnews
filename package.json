{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Implementação do https://www.tabnew.com.br para o https://curso.dev",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:down && npm run services:up && next dev",
    "only:dev": "next dev",
    "services:up": "docker compose -f infra/compose.yaml up --detach",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "nodemon -L --watch . --ext js,json --ignore node_modules --exec 'npm test'"
  },
  "author": "Lucas Ferreira",
  "license": "MIT",
  "dependencies": {
    "dependencia": "^1.0.0",
    "gh-pages": "^6.0.0",
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "18.2",
    "react-dom": "18.2"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.1.0"
  }
}
